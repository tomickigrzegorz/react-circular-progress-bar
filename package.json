{
  "name": "@tomik23/react-circular-progress-bar",
  "version": "0.1.0",
  "private": false,
  "main": "dist/index.js",
  "description": "A circular progress bar in svg",
  "author": "Grzegorz Tomicki",
  "dependencies": {
    "@storybook/addon-storyshots": "^6.3.2",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "react-test-renderer": "^17.0.2",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "clean": "rimraf dist",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "compile": "npm run clean && cross-env NODE_ENV=production babel src/components --out-dir dist --copy-files --ignore __tests__,spec.js,test.js,stories.js,__snapshots__",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "predeploy": "npm run build-storybook",
    "deploy-storybook": "gh-pages -d storybook-static",
    "build-storybook": "build-storybook -s public"
  },
  "files": ["dist"],
  "repository": {
    "type": "git",
    "url": "https://tomik23.github.io/react-circular-progress-bar"
  },
  "resolutions": {
    "babel-loader": "8.1.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "@storybook/addon-actions": "^6.3.2",
    "@storybook/addon-essentials": "^6.3.2",
    "@storybook/addon-links": "^6.3.2",
    "@storybook/node-logger": "^6.3.2",
    "@storybook/preset-create-react-app": "^3.1.7",
    "@storybook/react": "^6.3.2",
    "cross-env": "^7.0.3",
    "gh-pages": "^3.2.3"
  },
  "homepage": "https://tomik23.github.io/react-circular-progress-bar"
}
