{
  "name": "@tomik23/react-circular-progress-bar",
  "version": "0.1.15",
  "private": false,
  "license": "MIT",
  "main": "dist/index.js",
  "description": "A circular progress bar in svg",
  "author": "Grzegorz Tomicki",
  "scripts": {
    "predeploy": "npm run build-storybook",
    "deploy": "gh-pages -d storybook-static",
    "start": "npm run storybook",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "clean": "rimraf dist",
    "compile": "npm run clean && cross-env NODE_ENV=production babel src/components --out-dir dist --ignore '**/*.stories.js'",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/tomik23/react-circular-progress-bar"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "keywords": [
    "svg",
    "circular",
    "progress-bar",
    "animation"
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "homepage": "https://github.com/tomik23/react-circular-progress-bar",
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3"
  },
  "dependencies": {
    "prop-types": "^15.7.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "@storybook/addon-controls": "^6.3.2",
    "@storybook/addon-essentials": "^6.3.2",
    "@storybook/addon-storyshots": "^6.3.2",
    "@storybook/preset-create-react-app": "^3.1.7",
    "@storybook/react": "^6.3.2",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "babel-loader": "^8.2.2",
    "cross-env": "^7.0.3",
    "eslint-config-prettier": "^8.3.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint": "^7.30.0",
    "gh-pages": "^3.2.3",
    "intersection-observer": "^0.12.0",
    "prettier": "^2.3.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "react": "^17.0.2",
    "rimraf": "^3.0.2",
    "web-vitals": "^1.0.1"
  }
}
